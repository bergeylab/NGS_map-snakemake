module load gatk;java -jar -Xmx24g /storage/work/cxb585/bin/GenomeAnalysisTK-3.8-0-ge9d806836/GenomeAnalysisTK.jar -I data/aligned_reads/Col_Body_1.PE.bwa.sorted.fixed.filtered.postdup.RG.passed.mito_removed.bam -R genomes/p_humanus_reference.fa --filter_mismatching_base_and_quals -T IndelRealigner -targetIntervals data/aligned_reads/Col_Body_1.PE.bwa.sorted.fixed.filtered.postdup.RG.passed.mito_removed.bam.list -o data/aligned_reads/Col_Body_1.PE.bwa.sorted.fixed.filtered.postdup.RG.passed.mito_removed.local_realign.bam --log_to_file logs/Col_Body_1.local_realignment.log